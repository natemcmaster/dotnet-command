{
  "buildOptions": {
    "emitEntryPoint": true
  },
  "dependencies": {
    "BananaLauncher.Tool": {
      "version": "1.0.0-*",
      "type": "build"
    },
    "Microsoft.Data.Sqlite": "1.0.0",
    "Microsoft.EntityFrameworkCore.Tools": "1.0.0-preview2-final"
  },
  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0"
        }
      }
    },
    "net451": {}
  },
  "commands": {
    "banana": "BananaLauncher.Tool",
    "ef": "Microsoft.EntityFrameworkCore.Design",
    "update-db": "Microsoft.EntityFrameworkCore.Design --assembly bin/Debug/netcoreapp1.0/UserProject.dll database update --verbose"
  },
  "tools": {
    "dotnet-command": "1.0.0-*"
  }
}