{
  "version": "1.0.0-alpha-*",
  "description": "Extension for .NET Core CLI to run assemblies as commands on a project",
  "authors": [
    "Nate McMaster"
  ],
  "copyright": "MIT License",
  "packOptions": {
    "summary": "This extension for .NET Core CLI will find an assembly in the user build output and launch .NET Core using this assembly as an alternate entrypoint into the user application. This is especially useful for tools that need to load and execute user assemblies.",
    "repository": {
      "type": "git",
      "url": "https://github.com/natemcmaster/dotnet-command/"
    },
    "tags": [
      ".NET Core CLI",
      "commands",
      "project-json",
      "dotnet",
      "dotnet-command"
    ],
    "licenseUrl": "https://github.com/natemcmaster/dotnet-command/blob/master/LICENSE.md"
  },
  "buildOptions": {
    "emitEntryPoint": true,
    "outputName": "dotnet-command"
  },
  "dependencies": {
    "Microsoft.DotNet.Cli.Utils": "1.0.0-preview1-002702",
    "Microsoft.Extensions.CommandLineUtils": "1.0.0-rc2-final",
    "Microsoft.Extensions.DependencyModel": "1.0.0-rc2-final",
    "Newtonsoft.Json": "8.0.4-beta1",
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.0.0-rc2-3002702"
    }
  },
  "frameworks": {
    "netcoreapp1.0": {}
  }
}